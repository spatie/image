{
    "name": "spatie/image",
    "description": "Manipulate images with an expressive API",
    "keywords": [
        "spatie",
        "image"
    ],
    "homepage": "https://github.com/spatie/image",
    "license": "MIT",
    "authors": [
        {
            "name": "Freek Van der Herten",
            "email": "freek@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-exif": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "spatie/image-optimizer": "^1.8.0",
        "spatie/temporary-directory": "^2.3",
        "symfony/process": "^6.4|^7.4.3"
    },
    "require-dev": {
        "ext-gd": "*",
        "ext-imagick": "*",
        "jcupitt/vips": "^2.4.1",
        "laravel/sail": "^1.40",
        "pestphp/pest": "^4",
        "pestphp/pest-plugin-arch": "^4",
        "phpstan/phpstan": "^1.12.15",
        "spatie/pest-plugin-snapshots": "dev-main",
        "spatie/pixelmatch-php": "^1.1",
        "spatie/ray": "^1.44.1",
        "symfony/var-dumper": "^6.4.26|7.0"
    },
    "suggest": {
        "ext-gd": "To use the GD driver",
        "ext-imagick": "To use the Imagick driver",
        "jcupitt/vips": "To use the Vips driver"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Image\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Image\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "php -d zend.max_allowed_stack_size=-1 -d error_reporting=E_ALL\\&~E_DEPRECATED vendor/bin/pest",
        "analyse": "phpstan analyse --memory-limit=1G --no-progress",
        "baseline": "./vendor/bin/phpstan analyse --memory-limit=2G --generate-baseline"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
